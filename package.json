{
  "name": "@leomotors/music-bot",
  "version": "0.2.0",
  "description": "discord.js music bot components",
  "repository": "https://github.com/Leomotors/music-bot",
  "author": "Leomotors",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build-inner": "tsc && tsc -p tsconfig.bruh.json",
    "build": "yarn lm-config && yarn build-inner",
    "format": "prettier --write \"{src,tests}/**/*.ts\"",
    "lint": "eslint src && prettier --check \"{src,tests}/**/*.ts\"",
    "test": "node --es-module-specifier-resolution=node tests/index.js"
  },
  "devDependencies": {
    "@leomotors/scripts": "^3.1.0",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/node": "^17.0.25",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "eslint": "^8.13.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@discordjs/voice": "^0.9.0",
    "@yimura/scraper": "^1.2.3",
    "cocoa-discord-utils": "^1.6.0",
    "discord.js": "^13.6.0",
    "uuid": "^8.3.2",
    "ytdl-core": "^4.11.0"
  },
  "keywords": [
    "discord-bot",
    "music-bot"
  ]
}